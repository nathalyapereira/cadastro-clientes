{
  "version": 2,
  "builds": [
    {
      "src": "dist/cadastro-clientes/server/server.mjs",
      "use": "@vercel/node"
    },
    { "src": "dist/cadastro-clientes/browser/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "dist/cadastro-clientes/server/server.mjs" }
  ]
}
